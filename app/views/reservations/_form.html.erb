<%= form_for  @reservation, url: user_listing_reservations_path(current_user, @listing.id) do |f| %>
      <%= f.label "Select your date and time" %><br>

      <%= f.label "Start Date" %><br>
      <%= f.datetime_field :startdate %><br>

      <%= f.label "End Date" %><br>
      <%= f.datetime_field :enddate %><br>

      <%= f.submit "Park!"%>
<% end %>
