            <%= link_to "Sign in", sign_in_path %>
          <% end %></li>
        <li><% if signed_in? %>
        <%= link_to "Parking Spots", user_listings_path(current_user.id) %> 
        <% end %></li>
        <li><% if signed_in? %>
        <%= link_to "List Your Spot", new_user_listing_path(current_user.id) %> 
        <% end %></li>
      </ul>
      
      <ul class="nav navbar-nav navbar-right">
        <li > <% if signed_in? %>
